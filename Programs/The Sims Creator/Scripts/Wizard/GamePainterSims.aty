					GUISpacer()
					GUIButtonSims(
						Label="Paint"
						Action="Parent::GotoPaintScreen"
						ToolTip="Paint your Sim"
						EnabledTarget ="EnableNormalButtons"
					)
					GUISpacer()
					GUIButtonSims(
						Label="Save"
						Action="Parent::GotoSaveScreen"
						ToolTip="Save"
						EnabledTarget ="EnableNormalButtons"
					)
					GUISpacer()
					GUIButtonSims(
						Label="Exit"
						Action="Parent::ActivateHelpFrame Structure='GUIFragment:SimsExitDialog'"
						ToolTip="Exit"
						EnabledTarget ="EnableNormalButtons"
					)
					GUISpacer()
				)
			)
