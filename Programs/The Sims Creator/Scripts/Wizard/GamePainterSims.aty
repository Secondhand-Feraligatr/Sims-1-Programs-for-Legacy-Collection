				DisabledNormalImage="StartBarExit_rest"
				DisabledPressedImage="StartBarExit_pressed"
				DisabledTouchedNormalImage="StartBarExit_hover"
				Action="Parent::ExitGamesPainter"
				ToolTip="Exit"
