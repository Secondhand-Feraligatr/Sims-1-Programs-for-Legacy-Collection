RHWizGamePainterSims=RHXWizard(
	// All data used by the wizard is stored here
	Data=(
		TextureViewMode = "All"
		ModelViewMode = "All"
		ModelViewDirection = "Front"
		Blend = 0.5
		Tint = 0.5
		Stylize = 0.0
		Saturation = 0.5
		Mode = 0

		ImageScale = 0.5
		ImageRotate = 0.5	//0..1, 0.5=centered
		MaskWidth= 0.75
		MaskHeight= 0.96
		MaskStrength = 1.0
		MaskFeather = 0.3
		MaskOffsetX = 0.5
		MaskOffsetY = 0.43
		ProjectionLength = 0.3

		OpenGLOptionsEnabled = 0
		MainRenderer = 0 //0 = software 1=opengl

		WireFrame = 0
		ShowMask=0

		PaintMode=-1	// Solid color (0) or texture (1)?  -1 means not selected
		PaintTabMode = 0 //ColorTab (0) TextureTab (1)

		CurrentTextureSwatch = 0
		CurrentLogo = -1
		// Centered at 218, 47; width is 60, height is 40
		//	218/256 = 0.8515625
		//	47/256 = 0.18359375
		CurrentLogoX=0.8515625
		CurrentLogoY=0.18359375
		// 60/256 = 0.234375
		// 40/256 = 0.15625
		CurrentLogoScale = 0.2344
		CurrentLogoFlipped = 0

		CurrentGarment = 0
		EnableTextures = 1

		CurrentHeadModel = 0
		CurrentBodyModel = 1
		//CurrentHandsModel = 2
		CurrentLeftHandModel = 2
		CurrentRightHandModel = 2

		CurrentHeadShader = 0
		CurrentBodyShader = 1
		//CurrentHandShader = 2

		CurrentHeadPage = 1
		CurrentBodyPage = 1
		CurrentLogoPage = 1

		NumberOfHeadPages = 1
		NumberOfBodyPages = 1
		NumberOfLogoPages = 1

		TotalHeads = 0
		TotalBodies = 0
		TotalLogos = 0

		CurrentUpperGarmentPage = 1
		CurrentLowerGarmentPage = 1
		CurrentOtherGarmentPage = 1

		NumberOfUpperGarmentPages = 1
		NumberOfLowerGarmentPages = 1
		NumberOfOtherGarmentPages = 1

		TotalUpperGarments = 0
		TotalLowerGarments = 0
		TotalOtherGarments = 0

		SimCharacterFilterVersion = 0
		FileNameHead=UIString()
		FileNameBody=UIString()
		FileNameExtHead = ""
		FileNameExtBody = ""
		HeadSaveComplete="           "
		BodySaveComplete="           "

		PressedSound = "Pressed.wav"
		DeniedSound = "Denied.wav"

		EnableRadioButtons = 1 
		EnableNormalButtons = 1 

		ChildAdultRadio = 0
		MaleFemaleRadio = 0
		LgtMedDrkRadio = 0
	)

	EntryPoint=RHWizardStage(
		Action="GotoStartScreen"
	)

/*************************************************************************************************************************************************************************/
