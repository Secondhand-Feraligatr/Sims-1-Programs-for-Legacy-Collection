											GUISimsNumberChoice(Target="CurrentBodyPage" GridWidth=10 TargetMax="NumberOfBodyPages")
											GUISpacer(Width=0 Height = 10)
										)
									)
								)
							)
						//	GUISpacer(Height=10)
						)
						GUISpacer(Width=20)
					)
				)
			)
			
			//The 3D view of the sim
			GUIRegion(
				Manager=(
					Name="SimsRoundedBorder"
					Content=GUIRegion(		
						Layout=(Name="Ratio" Style="X" Ratio0=0.0 Ratio1=1.0 Ratio2=0.0)
						GUISpacer(Height = 5)
						GUIRegion(				
							Layout=(Name="Ratio" Style="Y" Ratio0=0.0 Ratio1=1.0 Ratio2=0.0)
							GUIRegion(
								Layout=(Name="Standard" Style="Y")
								GUISpacer(Height = 5)
								GUIRegion(
									Layout=(Name="Ratio" Style="X" Ratio0=1.0 Ratio1=0.0 Ratio2=1.0)
									GUISpacer(Width=0 Height = 0 )
									GUITextArea(Content='3D Preview' Font="SimsHelpTitleFont" TextColor=(Red=1.0 Blue=0.0 Green=1.0))						
									GUISpacer(Width=0 Height = 0 )
								)	
								GUISpacer(Height = 5)
							)
							GUIRegion(
								Manager=(Name="SimsDownBevelBorder"
									Content=GUIRegion(
										Layout=(Name="Singleton")
								
										WorldView(
											PopupMenuActive = 0
											ViewMode="3D"
											Background=SimsViewportBackgroundGradient()
											ZoomExtentsScale = 0.7
										)
						
									)
								)
							)
							GUIRegion(
								GUITextArea(Content='Left click and drag to rotate.' Font="SimsHelpBodyFont" TextColor=(Red=1.0 Blue=0.0 Green=1.0))						
								GUITextArea(Content='Right click and drag to zoom.' Font="SimsHelpBodyFont" TextColor=(Red=1.0 Blue=0.0 Green=1.0))						
								GUITextArea(Content='Middle or L and R click and drag to Pan.' Font="SimsHelpBodyFont" TextColor=(Red=1.0 Blue=0.0 Green=1.0))						
								GUITextArea(Content='Press R to reset the view.' Font="SimsHelpBodyFont" TextColor=(Red=1.0 Blue=0.0 Green=1.0))						
							)
						)
						GUISpacer(Height = 5)
					)
				)
			)
		)

		GUIRegion(
			Manager=(
				Name="SimsRoundedBorder"
				Content=GUIRegion(			
