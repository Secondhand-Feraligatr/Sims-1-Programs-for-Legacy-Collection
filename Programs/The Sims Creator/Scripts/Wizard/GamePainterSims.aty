						Manager=(
							Name="SimsRoundedBorder"
							Content=GUIRegion(
								Layout=(Name="Ratio" Style="Y" Ratio0=0.0 Ratio1=1.0 Ratio2=0.0  Ratio3=0.0 RespectChildRequests=1)
