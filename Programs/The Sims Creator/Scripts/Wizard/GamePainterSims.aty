							GUISpacer(Height=5)
							GUIImageRadioButton(
								DeselectedImage="SimsMale_rest"
								SelectedImage="SimsMale_pressed"
								TouchedDeselectedImage="SimsMale_hover"

								DisabledDeselectedImage="SimsMale_rest"
								DisabledSelectedImage="SimsMale_pressed"
								DisabledTouchedDeselectedImage="SimsMale_hover"
							
