			//The heads and bodies	
			GUIRegion(
				Manager=(
					Name="SimsRoundedBorder"
					RemoveSide = 0 //Remove left side
					Content=GUIRegion(
						Layout=(Name="Ratio" Style="X" Ratio0=0.0 Ratio1=1.0 Ratio2=0.0)
						GUISpacer(Width=20)
						GUIRegion(	
							//Layout=(Name="Ratio" Style="Y" Ratio0=0.0 Ratio1=1.0 Ratio2=0.0 Ratio3=1.0 Ratio4=0.0 Ratio5=0.0 RatioIdentifier="Number5")
							Layout=(Name="Equal" Style="Y" RatioIdentifier="Number5")
							GUIRegion(
								Manager=(
									Name="SimsRoundedBorder"
									Content=GUIRegion(		
										Layout=(Name="Ratio" Style="Y" Ratio0=0.0 Ratio1=1.0 Ratio2=0.0 )
										GUISpacer(Height=10)
										GUIRegion(
											GUISpacer(Width = 353 Height = 148 RiggingState="Client")
											Manager=(
												Name="SimsModelSelector" 
												NumberOfPages="NumberOfHeadPages" 
												CurrentPage="CurrentHeadPage" 
												Target="CurrentHeadModel" 
												Rows = 2
												Columns = 4
												Width = 353 
 												Height = 148
												SimCharacterFilterVersion="SimCharacterFilterVersion" 
												ModelType="Head"
												TotalModels="TotalHeads"
												ZoomScale = 1.1
											)
										)
										GUIRegion(
											Layout=(Name="Standard" Style="Y")
											GUISpacer(Width=0 Height = 10 )
											GUIRegion(
												Layout=(Name="Ratio" Style="X" Ratio0 = 1.0 Ratio1=0.0 Ratio2=0.0 Ratio3=0.0 Ratio4=1.0 Identifier="Number6")
												GUISpacer( Width=0 Height=0  RiggingState="Client")
												GUITextArea(Target="TotalHeads" TextColor=(Red=1.0 Blue =1.0 Green=1.0) FixedHeight=13 ReLayoutOnChange=1)
												GUISpacer( Width=4 Height=0 )
												GUITextArea(Content="Head Skins" TextColor=(Red=1.0 Blue =1.0 Green=1.0) FixedHeight=13)
												GUISpacer( Width=0 Height=0 )
											)		
											GUISimsNumberChoice(Target="CurrentHeadPage" TargetMax = "NumberOfHeadPages" )
											GUISpacer(Width=0 Height = 10 )
										)	
									)
								)
							)
						
							GUIRegion(
								Manager=(
									Name="SimsRoundedBorder"
									Content=GUIRegion(	
										Layout=(Name="Ratio" Style="Y" Ratio0=0.0 Ratio1=1.0 Ratio2=0.0 )
										GUISpacer(Height=10)
										GUIRegion(
											GUISpacer(Width = 353 Height = 162)
											Manager=(
												Name="SimsModelSelector" 
												NumberOfPages="NumberOfBodyPages" 
												CurrentPage="CurrentBodyPage" 
												Target="CurrentBodyModel" 
