		//Main Area
		GUIRegion(
			Layout=(Name="Ratio" Style="Y" Ratio0=0.0 Ratio1=1.0 Ratio2=0.0 RespectChildRequests=1 Identifier="Save Vertical Spacing")
			GUISpacer()
			GUIRegion(
